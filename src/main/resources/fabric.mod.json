{
  "schemaVersion": 1,
  "id": "mojangster",
  "version": "${version}",
  "name": "Mojangster",
  "description": "Mojangster allows you to have animated loading/reload overlays - configurable via ModMenu and has modpack support.",
  "authors": [
    "Tempora"
  ],
  "contact": {
    "sources": "https://github.com/temporadev/Mojangster/",
    "issues": "https://github.com/temporadev/Mojangster/issues"
  },
  "license": "MPL-2.0",
  "icon": "assets/mojangster/logo.png",
  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "i.am.cal.mojangster.Prelaunch"
    ],
    "modmenu": [
      "i.am.cal.mojangster.modmenu.ModMenuEntrypoint"
    ]
  },
  "accessWidener": "mojangster.aw",
  "mixins": [
    "mojangster.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.11.6",
    "minecraft": ["1.17.*", "1.18.*"]
  }
}
